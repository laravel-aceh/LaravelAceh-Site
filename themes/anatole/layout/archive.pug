extends partial/layout

block site_title
    !=  "Arsip . " + config.title

block description
    - var description = "Arsip - " + config.title;
    meta(name="description", content=description)

block content
    .autopagerize_page_element: .content
      .archive.animated.fadeInDown
        ul.list-with-title
          include mixins
          +postList()
          +make_pager(__('prev'), __('next'))
